                </action>
                <action name="fetchFilter" class="fetchFilterAction">
                        <result name="success">/jsp/user/products.jsp</result>
                </action>
                
                
                <action name="login" class="loginAction">
                        <result name="success">/jsp/admin/balance.jsp</result>
